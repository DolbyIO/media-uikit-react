    "sass": "^1.53.0",
    "sass-loader": "10.1.1",
    "storybook-css-modules": "^1.0.8",
    "storybook-design-token": "^2.0.0",
    "style-loader": "2.0.0",
    "testcafe": "^1.18.5",
    "ts-jest": "27.0.5",
    "ts-loader": "^9.3.1",
    "typedoc": "^0.22.11",
    "typedoc-plugin-markdown": "^3.11.12",
    "typescript": "~4.5.2",
    "url-loader": "^3.0.0",
